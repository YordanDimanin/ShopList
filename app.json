{
  "expo": {
    "name": "CartMate",
    "slug": "CartMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cartmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#25292E",
      "statusBar": {
        "style": "light"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#25292E"
      },
      "backgroundColor": "#25292E",
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "statusBar": {
        "backgroundColor": "#25292E",
        "barStyle": "light-content"
      },
      "package": "com.skamaz1000.CartMate"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#25292E",
          "dark": {
            "backgroundColor": "#25292E"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4edfa2c7-eabd-49c9-8695-03d223738a29"
      }
    }
  }
}
